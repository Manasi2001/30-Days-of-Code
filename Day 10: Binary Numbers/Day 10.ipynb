        "n = int(input())\r\n",
        "rmd = []\r\n",
        "while n > 0:\r\n",
        "    rm = n % 2\r\n",
        "    n = n//2\r\n",
        "    rmd.append(rm)\r\n",
        "count,result = 0,0\r\n",
        "for i in range(0,len(rmd)):\r\n",
        "    if rmd[i] == 0:\r\n",
        "        count = 0\r\n",
        "    else:\r\n",
        "        count +=1\r\n",
        "        result = max(result,count)\r\n",
        "print(result)   "
